{"legislative_session": "104th", "identifier": "SB135", "title": "EDGE-TRANSFER CREDIT", "from_organization": "~{\"classification\": \"upper\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Economic Development for a Growing Economy Tax Credit Act. Provides that the recipient of a credit under the Act may apply for a certificate of transferability of credit from the Department of Commerce and Economic Opportunity for the amount of the credit not previously claimed. Provides that the transferability certificate may be transferred or sold by the recipient to another Illinois taxpayer. Provides that unused Economic Development for a Growing Economy tax credits may be carried forward for a period of 10 years (currently, 5 years). Makes other changes."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with Secretary by Sen. Sally J. Turner", "date": "2025-01-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Assignments", "date": "2025-01-17", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Revenue", "date": "2025-01-28", "organization_id": "~{\"classification\": \"upper\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Rule 2-10 Committee Deadline Established As April 11, 2025", "date": "2025-03-21", "organization_id": "~{\"classification\": \"upper\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Sally J. Turner", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"upper\", \"name\": \"Sally J. Turner\"}", "organization_id": null}],