{"legislative_session": "104th", "identifier": "HB2589", "title": "HYPODERMIC NEEDLES-SALE", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Hypodermic Syringes and Needles Act. Provides that, upon request, a pharmacist shall (rather than may) sell up to 100 sterile hypodermic syringes or needles to a person who is at least 18 years of age. Provides that a syringe or needle sold at a pharmacy shall (rather than may) be sold only from the pharmacy department of the pharmacy."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Kelly M. Cassidy", "date": "2025-02-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Public Health Committee", "date": "2025-03-04", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Diane Blair-Sherlock", "date": "2025-03-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Jennifer Gong-Gershowitz", "date": "2025-03-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Jehan Gordon-Booth", "date": "2025-03-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Justin Slaughter", "dat