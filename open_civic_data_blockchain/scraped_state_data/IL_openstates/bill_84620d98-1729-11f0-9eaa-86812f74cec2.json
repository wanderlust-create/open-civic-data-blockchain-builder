{"legislative_session": "104th", "identifier": "HR113", "title": "DISTRACTED DRIVING AWARE-MNTH", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["resolution"], "subject": [], "abstracts": [{"note": "", "abstract": "Declares April 2025 as Distracted Driving Awareness Month in the State of Illinois. Encourages Illinois residents to use Distracted Driving Awareness Month as an opportunity to educate drivers of all ages on the dangers of distracted driving, to promote awareness campaigns through schools, workplaces, and community organizations, to encourage state and local law enforcement to continue enforcing distracted driving laws, and to highlight personal stories and testimonials of those impacted by distracted driving to inspire change."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Amy Elik", "date": "2025-02-05", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-06", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Amy Elik", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Amy Elik\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HR/PDF/10400HR0113.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=200673&DocName=10400HR0113&DocNum=113&DocTypeID=HR&LegID=160886&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https://beta.ilga.gov/Legislation/BillStatus?DocNum=113&GAID=18&DocTypeID=HR&LegId=160886&SessionID=114", "n