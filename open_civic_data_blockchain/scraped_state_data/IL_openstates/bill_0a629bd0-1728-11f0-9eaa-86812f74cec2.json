{"legislative_session": "104th", "identifier": "HB1256", "title": "WORKERS COMP-COMPOUND DRUGS", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Amends the Workers' Compensation Act in relation to custom compound medications. Sets forth conditions for approval of payment. Provides that charges shall be based upon the specific amount of each component drug and its original manufacturer's National Drug Code number and also upon specified criteria. Provides that a provider may prescribe a one-time 7-day supply unless a prescription for more than 7 days is preauthorized by the employer. Effective immediately."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Dan Ugaste", "date": "2025-01-10", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-01-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-01-28", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}], "sponsorships": [{"name": "Dan Ugaste", "classification": "primary", "entity_type": "person", "primary": true, "person_id": "~{\"chamber\": \"lower\", \"name\": \"Dan Ugaste\"}", "organization_id": null}], "related_bills": [], "versions": [{"note": "Introduced", "links": [{"url": "https://beta.ilga.gov/documents/legislation/104/HB/PDF/10400HB1256.pdf", "media_type": "application/pdf"}, {"url": "https://beta.ilga.gov/Legislation/BillStatus/FullText?LegDocId=196839&DocName=10400HB1256&DocNum=1256&DocTypeID=HB&LegID=157048&GAID=18&SessionID=114&SpecSess=&Session=&Print=1", "media_type": "text/html"}], "date": "", "classification": ""}], "documents": [], "citations": [], "sources": [{"url": "https: