{"legislative_session": "104th", "identifier": "HB3409", "title": "COSMETIC PRODUCTS ACT", "from_organization": "~{\"classification\": \"lower\"}", "classification": ["bill"], "subject": [], "abstracts": [{"note": "", "abstract": "Creates the Chemicals in Cosmetic Products Act. Prohibits the manufacture, sale, delivery, holding, or offering for sale of a cosmetic product that contains specific intentionally added chemical ingredients. Exempts cosmetic products that were manufactured with an intent to comply with the Act and contain a technically unavoidable trace quantity of a prohibited chemical ingredient due to certain reasons. Defines terms."}], "other_titles": [], "other_identifiers": [], "actions": [{"description": "Filed with the Clerk by Rep. Sonya M. Harper", "date": "2025-02-07", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["filing"], "related_entities": []}, {"description": "First Reading", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["reading-1"], "related_entities": []}, {"description": "Referred to Rules Committee", "date": "2025-02-18", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Assigned to Public Health Committee", "date": "2025-03-11", "organization_id": "~{\"classification\": \"lower\"}", "classification": ["referral-committee"], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Lisa Davis", "date": "2025-03-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Added Co-Sponsor Rep. Kimberly Du Buclet", "date": "2025-03-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"description": "Added Chief Co-Sponsor Rep. La Shawn K. Ford", "date": "2025-03-19", "organization_id": "~{\"classification\": \"lower\"}", "classification": [], "related_entities": []}, {"d